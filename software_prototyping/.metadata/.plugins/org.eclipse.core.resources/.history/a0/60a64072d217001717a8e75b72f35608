#ifndef LANDER_OS_H_
#define LANDER_OS_H_

typedef unsigned char cmd;

// pop the top command from the stack
cmd pop(cmd* head);

// push new command to the bottom of the stack
push(cmd ,cmd* tail);

initial_config();

landing_config();

#endif /* LANDER_OS_H_ */
