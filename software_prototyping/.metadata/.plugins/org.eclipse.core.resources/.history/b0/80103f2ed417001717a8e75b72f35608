cmd pop(cmd* head){
    if (head == tail){
        return 0; // indicate error
    } else{
        head = (head++) & (BIT2|BIT1|BIT0); // increment and wrap to 0-7
        return &head;
    }
}

cmd push(cmd data, cmd* tail){
    tail = (tail++) & (BIT2|BIT1|BIT0); // increment and wrap to 0-7
}
