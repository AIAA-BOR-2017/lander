#ifndef LANDER_OS_H_
#define LANDER_OS_H_

typedef unsigned char cmd;

// consts
#define CMD_QUEUE_LEN (8);
#define ALT_DIFF_THRESH (-100); // difference in altitude to detect descent

// pop the top command from the stack
cmd pop(cmd* head);

// push new command to the bottom of the stack
push(cmd data,cmd* tail);


initial_config();
landing_config();

wait_for_landing();

landing_ISR();

#endif /* LANDER_OS_H_ */
