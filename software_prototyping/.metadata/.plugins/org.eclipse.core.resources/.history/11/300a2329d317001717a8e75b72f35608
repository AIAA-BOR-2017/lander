#ifndef LANDER_OS_H_
#define LANDER_OS_H_

typedef unsigned char cmd;

// consts
#define CMD_QUEUE_LEN 10

// pop the top command from the stack
cmd pop(cmd* head);

// push new command to the bottom of the stack
push(cmd ,cmd* tail);


initial_config();
landing_config();

wait_for_landing();

#endif /* LANDER_OS_H_ */
